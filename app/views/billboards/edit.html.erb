<h1>Edit it up</h1>
<div class="container">
  <div class="row">
    <%= form_for @billboard do |f| %>
      <div class="col m6">
        <%= f.label :name %>
        <%= f.text_field :name %>
      </div>
      <div class="col m6">
        <%= f.label :total %>
        <%= f.number_field :total, min: 0, max: 25 %>
      </div>
      <%= f.submit class: "btn black white-text" %>
    <% end %>
  </div>
</div>
